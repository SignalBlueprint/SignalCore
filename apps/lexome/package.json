{
  "name": "lexome",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm:dev:server\" \"pnpm:dev:client\"",
    "dev:server": "tsx src/server.ts",
    "dev:client": "vite",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.json",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@sb/auth": "workspace:*",
    "@sb/storage": "workspace:*",
    "@sb/schemas": "workspace:*",
    "@sb/events": "workspace:*",
    "@sb/suite": "workspace:*",
    "@sb/cache": "workspace:*",
    "@sb/config": "workspace:*",
    "@sb/ai": "workspace:*",
    "@sb/telemetry": "workspace:*",
    "express": "^4.18.2",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "vite": "^5.0.8"
  }
}
