{
  "name": "lexome",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm:dev:server\" \"pnpm:dev:client\"",
    "dev:server": "tsx src/server.ts",
    "dev:client": "vite",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.json",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@sb/ai": "workspace:*",
    "@sb/auth": "workspace:*",
    "@sb/cache": "workspace:*",
    "@sb/config": "workspace:*",
    "@sb/events": "workspace:*",
    "@sb/schemas": "workspace:*",
    "@sb/storage": "workspace:*",
    "@sb/suite": "workspace:*",
    "@sb/telemetry": "workspace:*",
    "axios": "^1.6.2",
    "epubjs": "^0.3.93",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.21",
    "@types/jszip": "^3.4.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
